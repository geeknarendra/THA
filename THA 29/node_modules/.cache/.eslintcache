.js",[],"/media/noob/Data_File❤❤/Git Pushed/THA/THA 29/src/store.js",[],"/media/noob/Data_File❤❤/Git Pushed/THA/THA 29/src/App.jsx",["31"],"import { useDispatch, useSelector } from \"react-redux\";\nimport { addCity, addCityData } from \"./actions\";\nimport \"./App.css\";\n\nfunction App() {\n  const city = useSelector((state) => state.addCity);\n  const cityData = useSelector((state) => state.addCityData);\n  const dispatch = useDispatch();\n\n  return (\n    <div className=\"app\">\n      <header>\n        {cityData.location ? (\n          <div className=\"col\">\n            <img src={cityData.current.condition.icon} id=\"status\" />\n            <h>{cityData.current.temp_c} °C</h>\n          </div>\n        ) : (\n          <></>\n        )}\n        <div className=\"col\">\n          <input\n            id=\"input\"\n            type=\"text\"\n            placeholder=\"enter city\"\n            value={city}\n            onChange={(e) => {\n              dispatch(addCity(e.target.value));\n            }}\n            onKeyPress={(e) => {\n              if (e.key === \"Enter\") {\n                dispatch(addCityData(city));\n                dispatch(addCity(\"\"));\n              }\n            }}\n          />\n\n          <button\n            id=\"submit\"\n            onClick={() => {\n              dispatch(addCityData(city));\n              dispatch(addCity(\"\"));\n            }}\n          >\n            go\n          </button>\n\n          {cityData.location ? (\n            <div>\n              <h>{cityData.location.name}</h>\n            </div>\n          ) : (\n            <h id=\"error\">No Place Found</h>\n          )}\n        </div>\n      </header>\n\n      {cityData.location ? (\n        <div className=\"wrapper\">\n          <div className=\"cube\">\n            <div className=\"cube-row\">\n              <h1>description</h1>\n              <ion-icon name=\"information-circle\"></ion-icon>\n            </div>\n            <div className=\"cube-row\">\n              <p>{cityData.current.condition.text}</p>\n            </div>\n          </div>\n          <div className=\"cube\">\n            <div className=\"cube-row\">\n              <h1>feels like</h1>\n              <ion-icon name=\"thermometer\"></ion-icon>\n            </div>\n            <div className=\"cube-row\">\n              <p id=\"feels-like\">{cityData.current.feelslike_c}°C</p>\n            </div>\n          </div>\n          <div className=\"cube\">\n            <div className=\"cube-row\">\n              <h1>humidity</h1>\n              <ion-icon name=\"water\"></ion-icon>\n            </div>\n            <div className=\"cube-row\">\n              <p id=\"humidity\">{cityData.current.humidity}</p>\n            </div>\n          </div>\n          <div className=\"cube\">\n            <div className=\"cube-row\">\n              <h1>wind speed</h1>\n              <ion-icon name=\"rainy\"></ion-icon>\n            </div>\n            <div className=\"cube-row\">\n              <p id=\"wind-speed\">{cityData.current.wind_kph}kph</p>\n            </div>\n          </div>\n        </div>\n      ) : (\n        <></>\n      )}\n    </div>\n  );\n}\n\nexport default App;\n","/media/noob/Data_File❤❤/Git Pushed/THA/THA 29/src/reducers/index.js",[],"/media/noob/Data_File❤❤/Git Pushed/THA/THA 29/src/actions/index.js",[],"/media/noob/Data_File❤❤/Git Pushed/THA/THA 29/src/reducers/addCityReducer.js",[],"/media/noob/Data_File❤❤/Git Pushed/THA/THA 29/src/reducers/addCityDataReducer.js",[],{"ruleId":"32","severity":1,"message":"33","line":15,"column":13,"nodeType":"34","endLine":15,"endColumn":70},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement"]